            "diff_hunk": "@@ -692,6 +692,22 @@ describe('API: get highlight', function()\n     api.nvim_set_hl(0, 'Bar', { link = 'Foo', default = true, force = true })\n     eq({ link = 'Foo', default = true }, api.nvim_get_hl(0, { name = 'Bar' }))\n   end)\n+\n+  it('can set default link even if link is set', function()\n+    api.nvim_set_hl(0, 'From', { link = 'To1' })\n+    api.nvim_set_hl(0, 'From', { link = 'To2', default = true })\n+    eq({ link = 'To1' }, api.nvim_get_hl(0, { name = 'From' }))\n+    command('hi clear')\n+    eq({ link = 'To2' }, api.nvim_get_hl(0, { name = 'From' }))\n+  end)\n+\n+  it('does not overwrite default link', function()\n+    api.nvim_set_hl(0, 'From', { link = 'To1', default = true })\n+    api.nvim_set_hl(0, 'From', { link = 'To2', default = true })\n+    eq({ link = 'To1', default = true }, api.nvim_get_hl(0, { name = 'From' }))",
