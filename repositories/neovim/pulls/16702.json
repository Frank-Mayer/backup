    "comment_regular_data": [
        {
            "author_association": "CONTRIBUTOR",
            "body": "I don't like shimming the plaintext into markup content. I'd rather we just separately handled the case of plaintext.",
            "created_at": "2021-12-19T17:18:59Z",
            "html_url": "https://github.com/neovim/neovim/pull/16702#issuecomment-997428533",
            "id": 997428533,
